#m_local - \320\272\320\260\320\272\320\276\320\271 &quot;\321\203\320\263\320\276\320\273&quot; \320\275\320\260\320\264\320\276 \320\262\321\213\321\200\320\265\320\267\320\260\321\202\321\214 \320\270\320\267 \321\200\320\260\320\267\320\275\320\276\321\201\321\202\320\275\320\276\320\263\320\276 \321\203\321\200\320\260\320\262\320\275\320\265\320\275\320\270\321\217
#(\320\262\320\265\321\201\321\214 \321\205\320\260\321\200. \320\274\320\275\320\276\320\263. \320\270\321\211\320\265\320\274 ChPol(c,[-1,-1]);)
</Font>ChPol := proc(c_local, m_local)
local i,j,R;
#option trace;
R:=0;
for i from 0 to m[1] do
    for j from 0 to m[2] do
        if not(i&lt;=m_local[1] and j&lt;=m_local[2]) then 
           R:=R+c_local[i+1,j+1]*z^(i)*w^(j) 
        fi; 
    od;
od;
return(R);
end proc:</Text-field>
